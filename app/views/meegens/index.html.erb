<h1>Listing meegens</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @meegens.each do |meegen| %>
  <tr>
    <td><%=h meegen.name %></td>
    <td><%= link_to 'Show', meegen %></td>
    <td><%= link_to 'Edit', edit_meegen_path(meegen) %></td>
    <td><%= link_to 'Destroy', meegen, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New meegen', new_meegen_path %>